#include<iostream>
using namespace std;

int main()
{
    int Susu[5];
    int hasil=0;
    for(int i=0; i<5; i++)
        {
            cout << "Input Botol ke-" << (i + 1 ) << ":";
            cin >> Susu[i];
        }
     cout << "Nilai Susu yang Anda masukkan ..." << endl;
     for(int i=0; i<5; i++)
        {
            hasil+= Susu[i];
            cout << "\nBotol ke-" << (i + 1) << " adalah " << Susu[i] << " Liter";
        }
     cout << "\nTotal Susu setiap Botol ..." << endl << endl;
     cout << hasil << " Liter";
    return 0;
}
