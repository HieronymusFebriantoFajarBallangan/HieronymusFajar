#include <iostream>
using namespace std;
int main(){
int x;
int y;
int z;
cout<<"====Masukkan Bilangan X dan Y===="<< endl;
cout<<" X =" <<endl;
cin>>x;
cout<<" Y =" <<endl;
cin>>y;
z= x+y;
if (z>=0){
    cout<<" Nilai z adalah bilangan positif"<<endl;
}
else(z<0);{
cout<<"Nilai z adalah bilangan negatif"<<endl;
}
cout<<z<<endl;
cout<<"Program ini adalah program berhitung"<<endl;
return 0;
}
